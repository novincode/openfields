{
  "autoload": {
    "psr-4": {
      "OpenFields\\": "includes/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenFields\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs",
    "cbf": "phpcbf"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
